{
  "guildName": "Floor-Gang",
  "appName": "$t(guildName) Modmail",
  "cookie": "connect.sid",
  "urls": {
    "base": "/api",
    "discordBase": "https://discord.com/api",
    "oauth": "$t(urls.base)/oauth{{redirect}}",
    "logout": "$t(urls.base)/logout",
    "authenticate": "$t(urls.base)/self",
    "categories": "$t(urls.base)/categories",
    "categoriesOne": "$t(urls.base)/categories/{{category}}",
    "threads": "$t(urls.base)/categories/{{category}}/threads",
    "threadsOne": "$t(urls.base)/categories/{{category}}/threads/{{thread}}",
    "fetchThreadsByUser": "$t(urls.base)/categories/{{category}}/users/{{user}}/history",
    "fetchMember": "$t(urls.base)/categories/{{category}}/users/{{member}}",
    "fetchMembers": "$t(urls.base)/categories/{{category}}/members",
    "fetchDiscordMember": "$t(urls.discordBase)/users/@me",
    "fetchRole": "$t(urls.base)/categories/{{category}}/roles/{{role}}",
    "fetchChannel": "$t(urls.base)/categories/{{category}}/channels/{{channel}}"
  },
  "drawer": {
    "tabs": {
      "categories": "Categories",
      "threads": "Threads"
    },
    "threadListItem": {
      "newLabel": "New!",
      "respondedByLabel": "Last Response By",
      "createdDateLabel": "Thread Created At",
      "new": "This thread has new messages!",
      "replied": "This thread has been replied to.",
      "important": "This thread requires attention.",
      "question": "This thread requires attention."
    }
  },
  "message": {
    "unknownRole": "&lt;Unknown Role: '{{role}}'&gt;",
    "unknownChannel": "&lt;Unknown Channel: '{{channel}}'&gt;",
    "nameTooltip": "Click to view <b>{{user}}</b>'s history",
    "chips": {
      "internal": "Internal",
      "creator": "Creator",
      "deleted": "Removed"
    }
  },
  "errors": {
    "splash": {
      "noSelector": "You must specify a selector to use splash",
      "selectorNotFound": "The selected you entered could not be found in DOM."
    }
  }
}